{
	"name": "dotink/inkwell-doctrine",
	"description": "Doctrine 2 Integration for inKWell",
	"keywords": ["orm", "database", "doctrine"],
	"license": "AGPL-3.0",
	"type": "opus-package",
	"authors": [
		{
			"name": "Matthew J. Sahagian",
			"email": "msahagian@dotink.org",
			"role": "Lead Architect / Developer"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.dotink.org"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"doctrine/orm": "2.5.*",
		"symfony/yaml": "2.*",
		"nette/php-generator": "2.4.*"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"extra": {
		"opus": {
			"inkwell/framework": {
				"plugin/bin" : "bin",
				"plugin/config"  : "config",
				"plugin/include" : "include"
			}
		}
	}
}
